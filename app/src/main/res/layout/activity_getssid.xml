<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.roboclub.androbot.getssid">
    <TextView
        android:id="@+id/textView2"
        android:layout_width="42dp"
        android:layout_height="35dp"
        android:layout_marginLeft="32dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="100dp"
        android:text="SSID1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="234dp"
        android:layout_height="35dp"
        app:layout_constraintLeft_toRightOf="@+id/textView2"
        android:layout_marginLeft="33dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="98dp"
        android:layout_marginStart="32dp" />

    <TextView
        android:id="@+id/SSID2"
        android:layout_width="42dp"
        android:layout_height="35dp"
        android:text="SSID2"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/SSID3"
        android:layout_width="42dp"
        android:layout_height="35dp"
        android:text="SSID3"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/SSID2"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/SSID4"
        android:layout_width="42dp"
        android:layout_height="35dp"
        android:text="SSID 4"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/SSID3"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="42dp"
        android:layout_height="35dp"
        android:text="SSID 5"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/SSID4"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Spinner
        android:id="@+id/spinner5"
        android:layout_width="234dp"
        android:layout_height="35dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/spinner4" />

    <Spinner
        android:id="@+id/spinner4"
        android:layout_width="234dp"
        android:layout_height="35dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/spinner3"
        app:layout_constraintLeft_toRightOf="@+id/SSID4"
        android:layout_marginLeft="32dp" />

    <Spinner
        android:id="@+id/spinner3"
        android:layout_width="234dp"
        android:layout_height="35dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/spinner2"
        app:layout_constraintLeft_toRightOf="@+id/SSID3"
        android:layout_marginLeft="32dp" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="234dp"
        android:layout_height="35dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/spinner"
        app:layout_constraintLeft_toRightOf="@+id/SSID2"
        android:layout_marginLeft="32dp" />

</android.support.constraint.ConstraintLayout>
